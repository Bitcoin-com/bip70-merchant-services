{
  "name": "bip70-merchant-services",
  "version": "0.1.1",
  "description": "Web GUI for bip70 merchant services",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist && rimraf lint-reports",
    "build:dev": "npm run clean:dist && webpack --env=dev --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && npm run test && webpack --env=prod --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "test": "karma start --no-auto-watch --single-run",
    "test:watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szerhusenBC/react-typescript-bootstrap-webpack-starter.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "webpack",
    "webpack2",
    "bootstrap",
    "less",
    "starter"
  ],
  "author": "Gabriel Cardona <gabriel@bitcoin.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@types/react": "~15.0.20",
    "@types/react-dom": "~0.14.23",
    "axios": "^0.21.1",
    "badger-components-react": "^0.10.0",
    "bootstrap": "~4.2.1",
    "bootstrap-less": "^3.3.8",
    "react": "^16.9.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-countdown-now": "^2.1.1",
    "react-dom": "^16.5.2",
    "react-spinners": "^0.5.12",
    "react-tooltip": "^3.10.0",
    "reactstrap": "^7.0.2",
    "slp-sdk": "^4.7.0",
    "styled-components": "^5.0.1",
    "websocket": "^1.0.29"
  },
  "devDependencies": {
    "@types/jasmine": "~2.5.46",
    "@types/node": "~6.0.60",
    "@types/react-addons-test-utils": "~0.14.17",
    "css-loader": "~0.27.3",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.10.1",
    "html-webpack-plugin": "~2.28.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.5.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.3",
    "less": "~2.7.2",
    "less-loader": "~4.0.2",
    "phantomjs-prebuilt": "~2.1.14",
    "react-addons-test-utils": "~15.4.2",
    "rimraf": "~2.6.1",
    "source-map-loader": "~0.2.0",
    "style-loader": "~0.16.0",
    "ts-loader": "~2.0.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "tslint-loader": "~3.4.3",
    "typescript": "~2.2.1",
    "url-loader": "~0.5.8",
    "webpack": "~2.2.1",
    "webpack-dev-server": "~2.4.2",
    "webpack-merge": "~4.1.0"
  }
}
