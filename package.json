{
  "name": "react-typescript-bootstrap-webpack-starter",
  "version": "0.0.1",
  "description": "A basic starter for a project build with ReactJS, Typescript, Webpack and Bootstrap",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist",
    "build": "npm run clean:dist && webpack --progress --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --progress --hot --inline --profile --colors --display-error-details --display-cached  --content-base src/",
    "test": "TBD"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szerhusenBC/react-typescript-bootstrap-webpack-starter.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "webpack",
    "bootstrap",
    "less",
    "starter"
  ],
  "author": "Stephan Zerhusen",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "ts-loader": "^0.9.1",
    "less": "^2.3.1",
    "style-loader": "^0.13.1",
    "less-loader": "^2.2.3",
    "css-loader": "^0.25.0",
    "url-loader": "^0.5.7",
    "file-loader": "^0.9.0",
    "source-map-loader": "^0.1.5",
    "html-webpack-plugin": "^2.22.0",
    "copy-webpack-plugin": "^3.0.1",
    "rimraf": "latest"
  },
  "dependencies": {
    "react": "^15.3.2",
    "@types/react": "^0.14.41",
    "react-dom": "^15.3.2",
    "@types/react-dom": "^0.14.18",
    "bootstrap": "^3.3.7"
  }
}
