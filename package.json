{
  "name": "react-typescript-bootstrap-webpack-starter",
  "version": "0.0.1",
  "description": "A basic starter for a project build with ReactJS, Typescript, Webpack and Bootstrap",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist",
    "dist": "npm run clean:dist && npm run test && webpack --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --progress --hot --inline --profile --colors --display-error-details --display-cached  --content-base src/",
    "test": "karma start",
    "test:watch": "npm run test --auto-watch --no-single-run",
    "test:debug": "node $NODE_DEBUG_OPTION ./node_modules/karma/bin/karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szerhusenBC/react-typescript-bootstrap-webpack-starter.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "webpack",
    "bootstrap",
    "less",
    "starter"
  ],
  "author": "Stephan Zerhusen",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "ts-loader": "^0.9.1",
    "less": "^2.3.1",
    "style-loader": "^0.13.1",
    "less-loader": "^2.2.3",
    "css-loader": "^0.25.0",
    "url-loader": "^0.5.7",
    "file-loader": "^0.9.0",
    "source-map-loader": "^0.1.5",
    "html-webpack-plugin": "^2.22.0",
    "copy-webpack-plugin": "^3.0.1",
    "rimraf": "latest",
    "react-addons-test-utils": "^15.3.2",
    "@types/react-addons-test-utils": "^0.14.15",
    "chai": "^3.5.0",
    "@types/chai": "^3.4.33",
    "mocha": "^3.1.2",
    "@types/mocha": "^2.2.32",
    "sinon": "^1.17.6",
    "@types/sinon": "^1.16.31",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sinon": "^1.0.5",
    "phantomjs-prebuilt": "^2.1.13",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "@types/react": "^0.14.41",
    "react-dom": "^15.3.2",
    "@types/react-dom": "^0.14.18",
    "bootstrap": "^3.3.7"
  }
}
